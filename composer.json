{
  "name": "max-dreamsoft/yii2-asset-converter",
  "description": "Less, Sass, and Scss converter for Yii2. No system requires. yii2-composer support, Less autoupdate, customizing of output directory",
  "type": "yii2-extension",
  "keywords": ["yii", "yii2", "asset", "converter", "less", "sass", "scss", "compass"],
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.13",
    "yiisoft/yii2-composer": "2.*",
    "oyejorge/less.php": "1.7.0.*",
    "richthegeek/phpsass": "1.*",
    "leafo/scssphp": "0.7.2",
    "yourilima/scssphp-compass": "1.*"
  },
  "autoload": {
    "psr-0": {
      "dreamsoft\\assetConverter\\": ""
    }
  },
  "target-dir": "dreamsoft/assetConverter"
}